package exemple_affichage;
import java.lang.String;
import java.lang.reflect.*;

public class Action {

	public String met;
	public String arg;
		
	Action() {	
	}
	
	Action(String cdt) {
		this.met = cdt;
	}
	
	public void exec_act() {
		Method method = this.met.getClass().getMethod(met, arg);
		return;
	}

}
